PORT=5173
STATIC_DIR=public

# ✅ Your Ably API key (server side)
# Get this from: https://ably.com/dashboard → Your App → API Keys
ABLY_API_KEY=your_ably_api_key_here

# ✅ Ably channel name (optional, defaults to "telemetry-dashboard-channel")
ABLY_CHANNEL_NAME=telemetry-dashboard-channel

# ✅ Your Supabase project URL
# Get this from: https://supabase.com/dashboard → Settings → API → Project URL
SUPABASE_URL=your_supabase_url_here

# ✅ Your Supabase anon key (safe to expose to frontend)
# Get this from: https://supabase.com/dashboard → Settings → API → Project API keys → anon/public
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ✅ Your Supabase service role key (server-side only - KEEP SECRET!)
# Get this from: https://supabase.com/dashboard → Settings → API → Project API keys → service_role
SUPABASE_SERVICE_ROLE=your_supabase_service_role_key_here

# Optional: limit rows when scanning for sessions
SESSIONS_SCAN_LIMIT=10000
